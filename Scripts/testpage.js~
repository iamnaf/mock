function concatenate(first, last)
{
	var full;
	
	full = first + last;
	return full;
}
function secondFunction()
{
	var result=concatenate('Zara', 'Ali');
	document.write(result);
}
function over() {
	alert("mouse over");
	}

function out(){
	alert("mouse out")
	}

function getConfirmation(){
	var retVal = confirm("Are you sure you want to end?");
	if (retVal == true){
		document.write("show result");
		return true;
	}else{
		document.write("not yet");
		return false;
		}
	}
